    "elementName": "haxcms-elements",
    "generator-wcfactory-version": "0.3.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/haxcms-elements.css",
      "scss": "src/haxcms-elements.scss",
      "html": "src/haxcms-elements.html",
